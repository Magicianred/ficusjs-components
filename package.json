{
  "name": "@ficusjs/components",
  "version": "1.0.0-alpha.4",
  "description": "A library of UI components built for FicusJS ",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist build",
    "build": "run-p build:*",
    "build:components": "node scripts/build-components.js",
    "build:css": "node scripts/build-css.js",
    "build:sass": "node scripts/build-sass.js",
    "build:sass-dist": "node scripts/build-sass.js --output-dir=dist",
    "build:dist": "run-p build:dist:*",
    "build:dist:js": "node scripts/build-components.js --output-dir=dist",
    "build:dist:css": "node scripts/build-css.js --output-dir=dist",
    "build:docs": "npm run sass && cross-env NODE_ENV=production eleventy --input=src --output=build",
    "predev": "rimraf tmp && npm run sass",
    "dev": "run-p watch:*",
    "watch:src": "live-server --watch=src --port=8888 --no-browser --cors src",
    "watch:tmp": "live-server --watch=tmp --port=8889 --no-browser --cors tmp",
    "watch:docs": "cross-env NODE_ENV=development eleventy --input=src --output=tmp --serve",
    "watch:sass": "chokidar \"src/**/*.scss\" -c \"run-p sass\"",
    "sass": "run-p sass:tmp:*",
    "sass:tmp:components": "node scripts/build-sass.js --output-dir=tmp",
    "sass:tmp:docs": "sass src/scss/ficus-docs.scss tmp/css/ficus-docs.css",
    "lint": "run-p lint:*",
    "lint:js": "standard \"src/**/*.js\"",
    "lint:sass": "stylelint \"src/scss/**/*.scss\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ficusjs/ficusjs-components.git"
  },
  "keywords": [
    "ficusjs",
    "frontend",
    "web-components",
    "ui-toolkit",
    "custom-elements"
  ],
  "author": "Matt Levy",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "@11ty/eleventy-navigation": "0.1.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.1.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-replace": "2.4.1",
    "clean-css": "5.1.1",
    "cross-env": "7.0.3",
    "chokidar-cli": "2.1.0",
    "eleventy-plugin-toc": "1.1.0",
    "glob-all": "3.2.1",
    "live-server": "1.2.1",
    "log-symbols": "4.0.0",
    "markdown-it": "12.0.4",
    "markdown-it-anchor": "7.1.0",
    "markdown-it-emoji": "2.0.0",
    "markdown-it-table-of-contents": "0.5.2",
    "markdown-to-txt": "1.0.1",
    "minify-html-literals": "1.3.2",
    "mkdirp": "1.0.4",
    "npm-run-all": "4.1.5",
    "postcss-cli": "8.3.1",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "rollup-plugin-minify-html-literals": "1.2.5",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.32.8",
    "slugify": "1.4.7",
    "standard": "16.0.3",
    "stylelint": "13.12.0",
    "yargs": "16.2.0"
  },
  "exports": {
    ".": {
      "import": "./dist/components/loader/index.js"
    }
  }
}
