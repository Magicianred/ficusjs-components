{
  "name": "ficusjs-components",
  "version": "0.1.0",
  "description": "A library of UI components built for FicusJS ",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:components": "node scripts/build-components.js",
    "build:docs": "cross-env NODE_ENV=production eleventy --input=src --output=build",
    "dev": "run-p watch:*",
    "watch:components": "live-server --watch=src --port=8888 --no-browser --cors src",
    "watch:docs": "cross-env NODE_ENV=development eleventy --input=src --output=tmp --serve",
    "watch:sass": "node-sass-chokidar ./src/sass --watch --recursive --output-style compressed --output ./src/css",
    "lint:sass": "stylelint \"src/sass/**/*.scss\""
  },
  "author": "Matt Levy",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@rollup/plugin-commonjs": "16.0.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@rollup/plugin-replace": "2.3.4",
    "cross-env": "7.0.2",
    "glob-all": "3.2.1",
    "live-server": "1.2.1",
    "log-symbols": "4.0.0",
    "minify-html-literals": "1.3.2",
    "node-sass": "4.14.1",
    "node-sass-chokidar": "1.5.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.32.1",
    "rollup-plugin-minify-html-literals": "1.2.5",
    "rollup-plugin-terser": "7.0.2",
    "standard": "16.0.0"
  }
}
