{
  "name": "ficusjs-components",
  "version": "0.1.0",
  "description": "A library of UI components built for FicusJS ",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:components": "node scripts/build-components.js",
    "build:sass": "node-sass --output-style compressed --output ./dist/css ./src/sass",
    "watch:sass": "node-sass-chokidar --include-path ./src/sass --watch --recursive --output-style compressed --output ./dist/css"
  },
  "author": "Matt Levy",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "glob-all": "3.2.1",
    "log-symbols": "4.0.0",
    "minify-html-literals": "1.3.2",
    "node-sass": "4.14.1",
    "node-sass-chokidar": "1.5.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.28.2",
    "rollup-plugin-minify-html-literals": "1.2.5",
    "rollup-plugin-terser": "7.0.2",
    "standard": "14.3.4"
  }
}
