<!DOCTYPE html>
{# <html lang="en" data-theme="dark"> #}
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>{{title}}</title>
  <link href="https://fonts.googleapis.com/css2?family=Ubuntu:wght@400;500;700&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" rel="stylesheet"> 
  <!-- component css START -->
  {% set css %}
    {% include "../components/css/content.css" %}
    {% include "../components/css/navigation.css" %}
  {% endset %}
  <style>
    {{css | safe}}
  </style>
  <!-- component css END -->
  {% cssPath cssFile | safe %}
  <link href="https://unpkg.com/prismjs@1.20.0/themes/prism-okaidia.css" rel="stylesheet">
</head>
<body>
  <main class="[ fd-doc-layout ] [ fu-pl-xxl fu-pr-xxl fu-pt-xxxl fu-pb-xxxl fu-flow ]">
    <article class="[ fd-doc-article ] [ fu-flow ]">
      {{ content | safe }}
      {% bootstrap modulePath | safe %}
      {% scriptPath demoScript | safe %}
    </article>
    {% if content | toc %}
    <aside class="sticky-aside">
      <div class="fd-toc">
        <span>Table of contents</span>
        {{ content | toc| safe }}
      </div>
    </aside>
    {% endif %}
  </main>
</body>
</html>
