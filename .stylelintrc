{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": [true, {
      "ignoreAtRules": ["function", "if", "each", "include", "mixin", "use", "forward"]
    }],
    "selector-type-no-unknown": [true, {
      "ignore": ["custom-elements"]
    }]
  }
}
